@prefix mrel: <http://id.loc.gov/vocabulary/relators/>.
@prefix scoro: <http://purl.org/spar/scoro/>.
@base <http://example.com/ns#>.
@prefix event: <http://purl.org/NET/c4dm/>.
@prefix place: <http://purl.org/ontology/>.
@prefix wlo: <http://purl.org/ontology/wo/>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix bibo: <http://purl.org/ontology/bibo/>.
@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix vcard: <http://www.w3.org/2006/vcard/>.
@prefix lswpm: <http://ontology.cybershare.utep.edu/ELSEWeb/>.
@prefix bevon: <http://rdfs.co/bevon/>.
@prefix rr: <http://www.w3.org/ns/r2rml#>.


<#Mapping1> a rr:TriplesMap;
	rml:logicalSource [
		rml:source "/home/sali/Person.csv";
		rml:referenceFormulation ql:CSV
	];
	rr:subjectMap [
		rr:template "http://example.com/{email}";
		rr:class lswpm:Nu
	];

	rr:predicateObjectMap [
		rr:predicate event:place;
		rr:objectMap [rml:reference "place"]
	];

	rr:predicateObjectMap [
		rr:predicate bevon:color;
		rr:objectMap [rml:reference "color"]
	];

	rr:predicateObjectMap [
		rr:predicate dcterms:isPartOf;
		rr:objectMap [rml:reference "name"]
	].


<#Mapping2> a rr:TriplesMap;
	rml:logicalSource [
		rml:source "/home/sali/Person.csv";
		rml:referenceFormulation ql:CSV
	];
	rr:subjectMap [
		rr:template "http://example.com/{place}";
		rr:class place:Desert
	];

	rr:predicateObjectMap [
		rr:predicate wlo:name;
		rr:objectMap [rml:reference "name"]
	];

	rr:predicateObjectMap [
		rr:predicate mrel:aui;
		rr:objectMap [rml:reference "email"]
	];

	rr:predicateObjectMap [
		rr:predicate scoro:withJobTitle;
		rr:objectMap [rml:reference "color"]
	].
